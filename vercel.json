{
  "version": 2,
  "name": "pu-star-site",
  "alias": ["nextstarinnovations.com", "www.nextstarinnovations.com"],
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "MONGODB_URI": "@mongodb-uri",
    "JWT_SECRET": "@jwt-secret",
    "DEESMSX_API_KEY": "@deesmsx-api-key",
    "DEESMSX_SECRET_KEY": "@deesmsx-secret-key",
    "DEESMSX_SENDER_NAME": "@deesmsx-sender-name",
    "CLOUDINARY_CLOUD_NAME": "@cloudinary-cloud-name",
    "CLOUDINARY_UPLOAD_PRESET": "@cloudinary-upload-preset"
  }
} 